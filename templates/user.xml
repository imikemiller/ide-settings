<templateSet group="user">
  <template name="pys" value="eval(\Psy\sh());" description="Pyshell" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="this" value="$this-&gt;$THING$" description="$this" toReformat="false" toShortenFQNames="true">
    <variable name="THING" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="svgi" value="const $ICON$Svg = (props)=&gt;(&#10;  &lt;SvgIcon {...props}&gt;&#10;    $PATH$&#10;  &lt;/SvgIcon&gt;&#10;)&#10;export const $ICON$I = $ICON$Svg;" description="SVG Icon" toReformat="false" toShortenFQNames="true">
    <variable name="ICON" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>